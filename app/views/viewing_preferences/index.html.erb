<h1>Listing viewing_preferences</h1>

<table>
  <thead>
    <tr>
      <th>Textdetaillevel</th>
      <th>Imagedetaillevel</th>
      <th>Charactertag</th>
      <th>Storylinetag</th>
      <th>Startdate</th>
      <th>Enddate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @viewing_preferences.each do |viewing_preference| %>
      <tr>
        <td><%= viewing_preference.textDetailLevel %></td>
        <td><%= viewing_preference.imageDetailLevel %></td>
        <td><%= viewing_preference.characterTag_id %></td>
        <td><%= viewing_preference.storylineTag_id %></td>
        <td><%= viewing_preference.startDate %></td>
        <td><%= viewing_preference.endDate %></td>
        <td><%= link_to 'Show', viewing_preference %></td>
        <td><%= link_to 'Edit', edit_viewing_preference_path(viewing_preference) %></td>
        <td><%= link_to 'Destroy', viewing_preference, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Viewing preference', new_viewing_preference_path %>
