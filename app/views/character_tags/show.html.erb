<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @character_tag.name %>
</p>

<p>
  <strong>Relationship:</strong>
  <%= @character_tag.relationship %>
</p>

<p>
  <strong>Image:</strong>
  <% if @character_tag.image_url != nil %>
  <img src=<%= @character_tag.image_url %> />
  <% end %>
</p>

<p>
  <strong>Relationshiptype:</strong>
  <% if @character_tag.relationshipType != nil %>
  <%= @character_tag.relationshipType.category %>
  <% end %>
</p>

<p>
  <strong>Nickname:</strong>
  <%= @character_tag.nickname %>
</p>

<%= link_to 'Edit', edit_character_tag_path(@character_tag) %> |
