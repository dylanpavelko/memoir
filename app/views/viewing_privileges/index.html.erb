<h1>Listing viewing_privileges</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Charactertag</th>
      <th>Storylinetag</th>
      <th>Startdate</th>
      <th>Enddate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @viewing_privileges.each do |viewing_privilege| %>
      <tr>
        <td><%= viewing_privilege.name %></td>
        <td><%= viewing_privilege.characterTag_id %></td>
        <td><%= viewing_privilege.storylineTag_id %></td>
        <td><%= viewing_privilege.startDate %></td>
        <td><%= viewing_privilege.endDate %></td>
        <td><%= link_to 'Show', viewing_privilege %></td>
        <td><%= link_to 'Edit', edit_viewing_privilege_path(viewing_privilege) %></td>
        <td><%= link_to 'Destroy', viewing_privilege, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Viewing privilege', new_viewing_privilege_path %>
